{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test:unit": "NODE_CONFIG_ENV=test && jest unit -c jest.config.js",
    "test:int": "NODE_CONFIG_ENV=test && jest int -c jest.config.int.js" ,
    "dev": "NODE_CONFIG_ENV=development && ts-node-dev --no-notify ./src",
    "compile": "shx rm -rf ./lib/ && tsc",
    "start": "npm run compile && node lib/",
    "lint": "eslint ./src --ext .js,.ts",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/*.{js,ts,json,yml}": [
      "prettier --write"
    ],
    "src/*.{js,ts}": [
      "eslint"
    ]
  },
  "_moduleAliases": {
    "@constants": "lib/constants",
    "@declarations": "lib/declarations",
    "@middleware": "lib/middleware",
    "@models": "lib/models",
    "@routes": "lib/routes",
    "@services": "lib/services",
    "@utils": "lib/utils"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.7.4",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "module-alias": "^2.2.2",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.0",
    "serve-favicon": "^2.5.0",
    "winston": "^3.7.2",
    "zod": "^3.17.10"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/config": "^0.0.41",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.0",
    "@types/sequelize": "^4.28.13",
    "@types/serve-favicon": "^2.5.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "sequelize-cli": "^6.4.1",
    "shx": "^0.3.4",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.5",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  }
}
